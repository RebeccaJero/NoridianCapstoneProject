@model TrackTaskItemsDb.Models.QuarterItem

@using (Html.BeginForm())
{



    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>QuarterItem</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.StartQuarterId, "Start Quarter", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("StartQuarter", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.StartQuarterId, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.EndQuarterId, "End Quarter", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("EndQuarter", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.EndQuarterId, "", new { @class = "text-danger" })
            </div>
        </div>



        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

@foreach (var item in ViewBag.TaskItemId.Items)
{

    <div>
        @Html.ActionLink("Back to Detail", "Details", "ItemDepartments", new { id = item }, null)
    </div>
}
